{
  "name": "back",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "yarn run watch:dev",
    "server": "node ./dist-server/bin/www",
    "dev": "npm-run-all build server",
    "prod": "SET NODE_ENV=production && npm-run-all build server",
    "transpile": "babel ./server --out-dir dist-server",
    "clean": "rimraf dist-server",
    "build": "npm-run-all clean transpile",
    "watch:dev": "nodemon",
    "manual": "rimraf dist-server && babel ./server --out-dir dist-server && node ./dist-server/bin/www"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "include": [
      "server",
      "server/config"
    ]
  },
  "nodemonConfig": {
    "exec": "yarn run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.8.6",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "rimraf": "^3.0.2",
    "sequelize": "^5.21.5"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "sequelize-cli": "^5.5.1"
  }
}
